<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DAFDBA"
    tools:context=".addTasks">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="410dp"
        android:layout_height="310dp"
        android:layout_marginTop="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="335dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:background="#DAFDBA"
                android:gravity="center"
                android:paddingVertical="30dp" />

            <FrameLayout
                android:layout_width="335dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="#9AEBA3"
                android:gravity="center"
                android:paddingVertical="30dp">

                <MultiAutoCompleteTextView
                    android:layout_width="match_parent"
                    android:layout_height="59dp"
                    android:text="@string/txt17"
                    android:textColor="#9AEBA3" />

            </FrameLayout>


            <TextView
                android:layout_width="335dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:background="#DAFDBA"
                android:gravity="center"
                android:paddingVertical="30dp" />

            <FrameLayout
                android:layout_width="335dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="#9AEBA3"
                android:gravity="center"
                android:paddingVertical="30dp">

                <MultiAutoCompleteTextView
                    android:layout_width="match_parent"
                    android:layout_height="59dp"
                    android:text="@string/txt18"
                    android:textColor="#9AEBA3" />

            </FrameLayout>

            <TextView
                android:layout_width="335dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:background="#DAFDBA"
                android:gravity="center"
                android:paddingVertical="30dp" />

            <FrameLayout
                android:layout_width="335dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="#9AEBA3"
                android:gravity="center"
                android:paddingVertical="30dp">

                <MultiAutoCompleteTextView
                    android:layout_width="match_parent"
                    android:layout_height="59dp"
                    android:text="@string/txt19"
                    android:textColor="#9AEBA3" />

            </FrameLayout>

            <TextView
                android:layout_width="335dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:background="#DAFDBA"
                android:gravity="center"
                android:paddingVertical="30dp" />

            <FrameLayout
                android:layout_width="335dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="#9AEBA3"
                android:gravity="center"
                android:paddingVertical="30dp">

                <MultiAutoCompleteTextView
                    android:layout_width="match_parent"
                    android:layout_height="59dp"
                    android:text="@string/txt20"
                    android:textColor="#9AEBA3" />

            </FrameLayout>

            <TextView
                android:layout_width="335dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:background="#DAFDBA"
                android:gravity="center"
                android:paddingVertical="30dp" />

            <FrameLayout
                android:layout_width="335dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="#9AEBA3"
                android:gravity="center"
                android:paddingVertical="30dp">

                <MultiAutoCompleteTextView
                    android:layout_width="match_parent"
                    android:layout_height="59dp"
                    android:text="@string/txt21"
                    android:textColor="#9AEBA3" />

            </FrameLayout>

            <TextView
                android:layout_width="335dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:background="#DAFDBA"
                android:gravity="center"
                android:paddingVertical="30dp" />

        </LinearLayout>


    </ScrollView>

    <View
        android:layout_width="407dp"
        android:layout_height="370dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="4dp"
        android:background="@drawable/bg2_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="409dp"
        android:layout_height="96dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="2dp"
        android:background="@drawable/bg2_view"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.bottomappbar.BottomAppBar
        android:id="@+id/bottomAppBar3"
        android:layout_width="376dp"
        android:layout_height="70dp"
        android:layout_gravity="bottom"
        android:backgroundTint="#45C4B0"
        app:backgroundTint="#A999CFD6"
        app:fabCradleRoundedCornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.535"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977"
        tools:ignore="BottomAppBar" />

    <ImageButton
        android:id="@+id/btn_add_nav"
        android:layout_width="66dp"
        android:layout_height="65dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="20dp"
        android:backgroundTint="#009688"
        android:contentDescription="@string/btn_add_nav"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/bottomAppBar3"
        app:layout_constraintTop_toTopOf="@+id/bottomAppBar3"
        app:srcCompat="@drawable/plus_11969" />

    <ImageButton
        android:id="@+id/btn_calendar_nav"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#45C4B0"
        android:contentDescription="@string/btn_calendar_nav"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/bottomAppBar3"
        app:layout_constraintEnd_toStartOf="@+id/btn_home_nav"
        app:layout_constraintTop_toTopOf="@+id/bottomAppBar3"
        app:srcCompat="@drawable/calendar_11066" />

    <ImageButton
        android:id="@+id/btn_home_nav"
        android:layout_width="64dp"
        android:layout_height="62dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#45C4B0"
        android:contentDescription="@string/btn_home_nav"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/bottomAppBar3"
        app:layout_constraintEnd_toStartOf="@+id/btn_profile_nav"
        app:layout_constraintTop_toTopOf="@+id/bottomAppBar3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/home_2502" />

    <ImageButton
        android:id="@+id/btn_profile_nav"
        android:layout_width="64dp"
        android:layout_height="62dp"
        android:layout_marginStart="302dp"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#45C4B0"
        android:contentDescription="@string/btn_profile_nav"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/bottomAppBar3"
        app:layout_constraintStart_toStartOf="@+id/bottomAppBar3"
        app:layout_constraintTop_toTopOf="@+id/bottomAppBar3"
        app:srcCompat="@drawable/user_4251" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="127dp"
        android:layout_height="34dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="304dp"
        android:text="@string/add_1"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="55dp"
        android:layout_height="63dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/user_profile_svgrepo_com" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="232dp"
        android:layout_height="32dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="262dp"
        android:text="@string/add_2"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gp2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:text="@string/add_3"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>